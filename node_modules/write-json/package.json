    "fs.createWriteStream",
    "fs.writeFile",
    "fs.writeFileSync",
    "json",
    "make-dir",
    "make-dirp",
    "mkdir",
    "mkdirp",
    "object",
    "path",
    "promise",
    "read",
    "stringify",
    "sync",
    "Uint8Array",
    "write"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
